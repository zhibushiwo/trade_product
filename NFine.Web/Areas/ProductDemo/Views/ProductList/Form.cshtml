    //数据验证
    function validate(obj) {
        obj.removeClass('form-controlError');
        var value = obj.val()
        if (value="") {
            obj.addClass('form-controlError');
            return false
        }
        return true;
    }
    //获取子表数据
    function geSubdate() {
        var txt = "[";
        var jsonArr = [];
        if ($("input[type='radio']:checked").val() == 2 || $("input[type='radio']:checked").val() == "2") {
            $("#grid #grid_body").find("tr").each(function () {
                var json = {};
                var flag = false;
                txt += "{"
                $(this).find("[name]").each(function () {
                    flag = true;
                    json[$(this).attr("name")] = this.value;
                });
                if (flag) {
                    jsonArr.push(json);
                }
            });
        } else {
            $("#radio_0 table").each(function () {
                var json = {};
                var flag = false;
                txt += "{"
                $(this).find("[name]").each(function () {
                    flag = true;
                    json[$(this).attr("name")] = this.value;
                });
                if (flag) {
                    jsonArr.push(json);
                }
            });
        }
        
        if (jsonArr.length <= 0) {
            $.modalMsg("数据异常", "error");
            return false;
        }
        var html = "[";
        for (var i = 0; i < jsonArr.length; i++) {
            html += JSON.stringify(jsonArr[i]) + ","
        }
        html = html.substring(0, html.length - 1)
        html += "]";
        return html;
     }
