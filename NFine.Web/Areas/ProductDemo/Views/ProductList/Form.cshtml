    //提交
    function submitForm() {
        if (!$('#form1').formValid()) {
            return false;
        }
        var json = geSubdate();
        var postData = $("#form1").formSerialize();
        postData["jsonDate"] = json;
        postData["HInterID"] = keyValue;
        postData["Producttype"] = $("input[type='radio']:checked").val();
        postData["ImgUrl"] = $("#ImgUrl [name='upload']").val();
        $.loading(true, "正在提交数据...");
        window.setTimeout(function () {
            if ($('[name=__RequestVerificationToken]').length > 0) {
                postData["__RequestVerificationToken"] = $('[name=__RequestVerificationToken]').val();
            }
            $.ajax({
                url: "/ProductDemo/ProductList/SubmitForm?keyValue=" + keyValue,
                data: postData,
                type: "post",
                dataType: "json",
                success: function (data) {
                    if (data.state == "success") {
                        window.parent.postMessage({ returnSuccess: true }, "*");
                        $.loading(false);
                        $.modalMsg(data.message, data.state);
                        location.reload();
                        $.modalClose();

                    } else {
                        $.modalAlert(data.message, data.state);
                    }
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    $.loading(false);
                    $.modalMsg(errorThrown, "error");
                },
                beforeSend: function () {
                    $.loading(true, "正在提交数据...");
                },
                complete: function () {
                    $.loading(false);
                }
            });
        }, 500);
