        } else {
            _html += '<tr>';
            _html += '    <td class="col1-mod" rowspan="1" style="border-right-style: hidden;;border-right: 1px solid #beaaaa;"><img src="/Content/img/samples/IMG_6537.png" style="width:100px; height:100px" /></td>';
