            _html += '    <td class="col8-mod" style="border-right-style: hidden">' + opj.Long + '*' + opj.Wide + '*' + opj.High + '</td>';
            _html += '    <td class="col9-mod" style="border-right-style: hidden">' + opj.HWeight + '</td>';
